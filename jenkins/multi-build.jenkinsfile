// Jenkinsfile (Scripted Pipeline)
node { // node/agent
    stage('Deploy') {
    	List<Map<String, String>> jobData = bom.toJobData(params.BOM)

    	jobData.each {
        	println("Building ${it.APPLICATION}-${it.VERSION}")
        	build(job: ${it.APPLICATION}, parameters: [string(name: 'GIT_REF', value: 'master'), string(name: 'VERSION', value: ${it.VERSION})])

    	}
    }
}